public class main{
public static void main(string[]args)}
scanner sc=new scanner(system.in);
int[][]a = new int [2] [3];
for(int i = 0;i < 2;i++){
for(int j = 0;j<3;j++){
a[i][j] = sc.nextint();
}
}
for(int j = 0;j<3;j++){
for(int i = 0; i<2;i++){
system.out.print(a[i][j] +" ");
}
system.out.println();
}
}
}
